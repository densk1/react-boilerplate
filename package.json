{
  "name": "react-bootstrap-crm",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.17.1",
    "csv": "^2.0.0",
    "html-entities": "^1.2.1",
    "moment": "^2.20.1",
    "node-sass-chokidar": "0.0.3",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-bootstrap": "^0.32.1",
    "react-bootstrap-modal": "^4.0.0",
    "react-dom": "^16.2.0",
    "react-dropzone": "^4.2.8",
    "react-file-reader": "^1.1.4",
    "react-moment": "^0.7.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-protected-route": "^1.0.0",
    "react-scripts": "1.0.17",
    "reactstrap": "^5.0.0-alpha.4",
    "redux": "^3.7.2",
    "redux-form": "^7.2.3",
    "redux-thunk": "^2.2.0"
  },
  "scripts": {
    "start": "concurrently -p [{index}][{name}] -c green.bold,blue.bold,magenta.bold,red.bold,yello.bold,red.bold -n \"CLIENT,SERVER, SASS , TEST \"  \"npm run client\" \"npm run server\" \"npm run watch-css\" \"npm run test\"",
    "build-css": "node-sass-chokidar src/ -o public/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o public/ --watch --recursive",
    "client": "react-scripts start",
    "server": "cd server && nodemon index.js",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "server": "nodemon ./server/index.js",
    "dev": "concurrently \"npm start\" \"npm server\" "
  },
  "proxy": "http://localhost:4000",
  "description": "Boilerplate for react project running with redux.",
  "main": "index.js",
  "devDependencies": {
    "concurrently": "^3.5.1",
    "eslint": "^4.19.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "install-peers": "^1.0.2",
    "morgan": "^1.9.0",
    "node-sass": "^4.7.2",
    "redux-logger": "^3.0.6",
    "sass-loader": "^6.0.6"
  },
  "author": "densk1",
  "license": "ISC"
}
